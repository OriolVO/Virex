// Generic utility functions example

extern func println_i32(i32 val) -> void;

// Generic max function
func max<T>(T a, T b) -> T {
    if (a > b) {
        return a;
    }
    return b;
}

// Generic min function  
func min<T>(T a, T b) -> T {
    if (a < b) {
        return a;
    }
    return b;
}

// Generic swap function
func swap<T>(T* a, T* b) -> void {
    unsafe {
        var T temp = *a;
        *a = *b;
        *b = temp;
    }
}

func main() -> i32 {
    // Test with integers
    var i32 x = max<i32>(10, 20);
    println_i32(x);  // Should print 20
    
    // Test with different type
    var i32 y = min<i32>(5, 3);
    println_i32(y);  // Should print 3
    
    return 0;
}
