import "io.vx";

func main() -> i32 {
    io.println("Testing Arithmetic:");
    
    var i32 a = 10;
    var i32 b = 3;
    
    io.print("10 + 3 = "); io.println(a + b);
    io.print("10 - 3 = "); io.println(a - b);
    io.print("10 * 3 = "); io.println(a * b);
    io.print("10 / 3 = "); io.println(a / b);
    io.print("10 % 3 = "); io.println(a % b);
    
    return 0;
}
