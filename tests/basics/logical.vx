import "io.vx";

func main() -> i32 {
    io.print("Testing Logical Operators:"); io.print("\n");
    
    var i32 a = 10;
    var i32 b = 20;
    
    io.print("10 < 20  : "); io.print(a < b); io.print("\n");
    io.print("10 > 20  : "); io.print(a > b); io.print("\n");
    io.print("10 <= 10 : "); io.print(10 <= 10); io.print("\n");
    io.print("10 >= 20 : "); io.print(a >= b); io.print("\n");
    io.print("10 == 10 : "); io.print(10 == 10); io.print("\n");
    io.print("10 != 20 : "); io.print(a != b); io.print("\n");
    
    return 0;
}
