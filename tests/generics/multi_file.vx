
module "multi_file";

import "lib.vx" as lib;
import "io.vx" as io;

func main() -> i32 {
    io.println("Testing multi-file generics...");

    var i32 x = 10;
    var i32 y = lib.identity<i32>(x);
    
    if (x != y) {
        io.println("FAIL: identity mismatch");
        return 1;
    }
    
    var i64 z = 100;
    var i64 w = lib.identity<i64>(z);
    
    if (z != w) {
        io.println("FAIL: identity i64 mismatch");
        return 1;
    }
    
    io.println("PASS: Multi-file generics work");
    return 0;
}
