module "printf_test";

// Declare external C function printf
extern func printf(cstring format, ...) -> i32;

func main() -> i32 {
    printf(cast<cstring>("Hello from Virex FFI!\n"));
    printf(cast<cstring>("Number: %d\n"), 42);
    printf(cast<cstring>("String: %s, Number: %d\n"), cast<cstring>("test"), 123);
    return 0;
}
