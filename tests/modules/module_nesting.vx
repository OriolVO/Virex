module "module_nesting_test";

import "io.vx";
import "inner_helper.vx" as inner;
import "deep/very_deep_helper.vx" as deep;

func main() -> i32 {
    io.print("Inner val: ");
    io.print(inner.get_val()); io.print("\n");
    
    io.print("Deep val: ");
    io.print(deep.deep_val); io.print("\n");
    
    if (inner.get_val() == 42 && deep.deep_val == 100) {
        io.print("Module nesting test passed!"); io.print("\n");
        return 0;
    }
    
    io.print("Module nesting test failed!"); io.print("\n");
    return 1;
}
