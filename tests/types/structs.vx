import "io.vx";

struct Point {
    i32 x;
    i32 y;
};

func main() -> i32 {
    io.print("Testing Structs:"); io.print("\n");
    
    var Point p;
    p.x = 10;
    p.y = 20;
    
    if (p.x == 10 && p.y == 20) {
        io.print("Struct members set correctly."); io.print("\n");
        io.print("p.x: "); io.print(p.x); io.print("\n");
        io.print("p.y: "); io.print(p.y); io.print("\n");
    } else {
        io.print("Error: Struct member mismatch."); io.print("\n");
        return 1;
    }
    
    return 0;
}
